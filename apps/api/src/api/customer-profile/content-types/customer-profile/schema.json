{
  "kind": "collectionType",
  "collectionName": "customer_profiles",
  "info": {
    "singularName": "customer-profile",
    "pluralName": "customer-profiles",
    "displayName": "Customer Profile"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "text"
    },
    "postcode": {
      "type": "string",
      "maxLength": 12
    },
    "phoneNumber": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "district": {
      "type": "string"
    },
    "precint": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "user_account": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "customer_profile"
    }
  }
}
